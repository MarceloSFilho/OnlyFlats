<p style="color: green"><%= notice %></p>

<h1>Flats</h1>

<div id="flats">
  <% @flats.each do |flat| %>
    <%= render flat %>
    <p>
      <%= link_to "Show this flat", flat, class:"btn-show-this-flat"%>
    </p>
  <% end %>
</div>

<%= link_to "New flat", new_flat_path, class:"btn-show-this-flat"%>
